{
  "name": "arduair2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "dev": "DEBUG=arduair:* nodemon --exec npm run babel-node -- ./src/bin/www",
    "babel-node": "babel-node --presets=es2017 ",
    "build": "babel src -d dist --copy-files",
    "mocha": "mocha --compilers js:babel-register",
    "test": "mocha --compilers js:babel-register --recursive ./src/test/ --timeout 10000 || true",
    "testing": "nodemon --exec 'mocha --watch --compilers js:babel-register --recursive ./src/test/ --timeout 10000'"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "mongoose": "^4.9.6",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.3.0",
    "supertest": "^3.0.0",
    "watch": "^1.0.2"
  }
}
